{
    "expo": {
        "name": "SCO - BRINKS",
        "slug": "Simulador-Operacional-BRINKS",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash_mdpi.png",
            "resizeMode": "cover"
        },
        "android": {
            "splash": {
                "mdpi": "./assets/splash_mdpi.png",
                "hdpi": "./assets/splash_hdpi.png",
                "ldpi": "./assets/splash_ldpi.png",
                "xhdpi": "./assets/splash_xhdpi.png",
                "xxhdpi": "./assets/splash_xxhdpi.png",
                "xxxhdpi": "./assets/splash_xxxhdpi.png",
                "resizeMode": "cover"
            }
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
